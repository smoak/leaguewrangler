{
  "name": "leaguewrangler",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/registerServiceWorker.ts",
      "!src/index.tsx",
      "!**/index.ts",
      "!src/**/*.stories.tsx"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 85,
        "functions": 85,
        "lines": 85,
        "statements": 85
      }
    }
  },
  "dependencies": {
    "@apollo/client": "^3.7.3",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "graphql": "^14.5.8",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-redux": "^7.2.4",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "TZ=UTC react-scripts test --env=jsdom --coverage --coverageDirectory=coverage",
    "eject": "react-scripts eject",
    "watch": "react-scripts test --env=jsdom",
    "build:graphql": "apollo client:codegen --includes=\"src/graphql/{mutations,queries}/*.ts\" --endpoint='https://leaguewrangler.com/graphql' --outputFlat --target=typescript src/graphql/types/",
    "storybook": "start-storybook -p 9009 -s public -c .storybook",
    "build-storybook": "build-storybook -s public"
  },
  "devDependencies": {
    "@babel/core": "^7.13.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "12.0",
    "@testing-library/react-hooks": "^3.2.1",
    "@testing-library/user-event": "^14.4.3",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.17.13",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^4.3.5",
    "@types/storybook__react": "^4.0.2",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "jest-enzyme": "^7.1.2",
    "jest-localstorage-mock": "^2.4.0",
    "prettier": "^2.3.2",
    "redux-devtools-extension": "^2.13.8",
    "ts-jest": "^26.5.6",
    "typescript": "^4.7.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "overrides": [
      {
        "files": [
          "**/*.ts?(x)"
        ],
        "rules": {
          "@typescript-eslint/explicit-function-return-type": "off"
        }
      }
    ]
  }
}
