{
  "name": "leaguewrangler",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/registerServiceWorker.ts",
      "!src/index.tsx",
      "!**/index.ts",
      "!src/**/*.stories.tsx"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "@material-ui/core": "^3.9.3",
    "@material-ui/icons": "^3.0.2",
    "@storybook/addon-info": "^5.1.10",
    "@types/moment-timezone": "^0.5.12",
    "@types/storybook__react": "^4.0.2",
    "apollo": "^2.17.1",
    "apollo-boost": "^0.3.1",
    "apollo-link-context": "^1.0.18",
    "awesome-typescript-loader": "^5.2.1",
    "graphql": "^14.4.2",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.26",
    "react": "^16.8.6",
    "react-apollo": "^2.5.8",
    "react-docgen-typescript-webpack-plugin": "^1.1.0",
    "react-dom": "^16.8.6",
    "react-moment": "^0.9.2",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.0.1",
    "react-scripts": "3.0.0",
    "ts-jest": "^24.0.2",
    "waait": "^1.0.5"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage --coverageDirectory=coverage",
    "eject": "react-scripts eject",
    "watch": "react-scripts test --env=jsdom",
    "build:graphql": "apollo client:codegen --includes=src/graphql/{mutations,queries}/*.ts --endpoint='https://teamcowboy-graphql.gigalixirapp.com/graphql' --outputFlat --target=typescript src/graphql/types/",
    "storybook": "start-storybook -p 9009 -s public -c .storybook",
    "build-storybook": "build-storybook -s public",
    "deployprep:diff": "git diff --name-only --exit-code || (echo \"There were uncommitted changes.\" && false)",
    "deployprep": "run-s -s deployprep:*",
    "deploy:gh-pages-cleanup": "node node_modules/gh-pages/bin/gh-pages-clean.js",
    "deploy:gh-pages": "gh-pages -d build -b production -m publish -r git@github.com:smoak/leaguewrangler.git",
    "deploy": "run-s build deployprep deploy:*"
  },
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@storybook/addon-actions": "^5.1.10",
    "@storybook/addon-links": "^5.1.10",
    "@storybook/addons": "^5.1.10",
    "@storybook/react": "^5.1.10",
    "@types/enzyme": "^3.10.3",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.14.14",
    "@types/react": "^16.8.24",
    "@types/react-dom": "^16.8.5",
    "@types/react-redux": "^7.1.1",
    "@types/react-router-dom": "^4.3.4",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.14.0",
    "gh-pages": "^2.1.0",
    "jest-enzyme": "^7.0.2",
    "jest-localstorage-mock": "^2.4.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.14.3",
    "react-test-renderer": "^16.8.6",
    "redux-devtools-extension": "^2.13.8",
    "typescript": "^3.5.3"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
