{
  "name": "leaguewrangler",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx}",
      "!**/node_modules/**",
      "!src/registerServiceWorker.ts",
      "!src/index.tsx",
      "!**/index.ts",
      "!src/**/*.stories.tsx"
    ],
    "coverageReporters": [
      "text",
      "text-summary",
      "html"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 100,
        "functions": 100,
        "lines": 100,
        "statements": 100
      }
    }
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "@material-ui/core": "^4.11.4",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.58",
    "apollo": "^2.21.2",
    "apollo-boost": "^0.4.7",
    "apollo-link-context": "^1.0.19",
    "awesome-typescript-loader": "^5.2.1",
    "graphql": "^14.5.8",
    "moment": "^2.24.0",
    "moment-timezone": "^0.5.27",
    "react": "^16.12.0",
    "react-apollo": "^3.1.0",
    "react-dom": "^16.12.0",
    "react-moment": "^0.9.7",
    "react-redux": "^6.0.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "4.0.3"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom --coverage --coverageDirectory=coverage",
    "eject": "react-scripts eject",
    "watch": "react-scripts test --env=jsdom",
    "build:graphql": "apollo client:codegen --includes=\"src/graphql/{mutations,queries}/*.ts\" --endpoint='https://leaguewrangler.com/graphql' --outputFlat --target=typescript src/graphql/types/",
    "storybook": "start-storybook -p 9009 -s public -c .storybook",
    "build-storybook": "build-storybook -s public",
    "deployprep:diff": "git diff --name-only --exit-code || (echo \"There were uncommitted changes.\" && false)",
    "deployprep": "run-s -s deployprep:*",
    "deploy:gh-pages-cleanup": "node node_modules/gh-pages/bin/gh-pages-clean.js",
    "deploy:gh-pages": "gh-pages -d build -b production -m publish -r git@github.com:smoak/leaguewrangler.git",
    "deploy": "run-s build deployprep deploy:*",
    "test:lint": "jest --config config/jest/lint.js"
  },
  "devDependencies": {
    "@apollo/react-testing": "^3.1.3",
    "@babel/core": "^7.13.0",
    "@storybook/addon-actions": "^6.2.9",
    "@storybook/addon-links": "^6.2.9",
    "@storybook/addons": "^6.2.9",
    "@storybook/react": "^6.2.9",
    "@testing-library/react-hooks": "^3.2.1",
    "@types/enzyme": "^3.10.4",
    "@types/enzyme-adapter-react-16": "^1.0.5",
    "@types/jest": "^23.3.14",
    "@types/node": "^10.17.13",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^4.3.5",
    "@types/storybook__react": "^4.0.2",
    "@typescript-eslint/eslint-plugin": "^4.0.0",
    "@typescript-eslint/parser": "^4.0.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "eslint-config-prettier": "^6.9.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-jest": "^22.21.0",
    "eslint-plugin-jest-formatting": "^1.2.0",
    "eslint-plugin-prettier": "^3.1.2",
    "gh-pages": "^2.2.0",
    "jest-enzyme": "^7.1.2",
    "jest-localstorage-mock": "^2.4.0",
    "jest-runner-eslint": "^0.10.0",
    "npm-run-all": "^4.1.5",
    "prettier": "1.14.3",
    "react-test-renderer": "^16.12.0",
    "redux-devtools-extension": "^2.13.8",
    "ts-jest": "^26.5.6",
    "typescript": "^3.8.0"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
